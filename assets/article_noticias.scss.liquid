html{
  padding-bottom: 0px !important;
}
body {
  padding: 15px 0;
}
.js-marquee p{
  margin: 0px !important;
}
.content-cintillo-loop-infografia{
  overflow: hidden !important;
  margin-top: -11px;
}
.border-top-0{
  border-top: none !important;
}
.border-bottom-0{
  border-bottom: none !important;
}
.medium-down--show{
  display: block !important;
}
#aviso-de-privacidad, #terminos-y-condiciones{
  .content-article{
    .container-box-general{
      background: url('https://cdn.shopify.com/s/files/1/0580/1695/6589/files/Fondo-negro.jpg?v=1629846635') !important;
      .title_lines{
        color: #fff !important;
        &:before, &:after{
          background-color: #FFF !important;
        }
      }
      .column-information-first{
        &::first-letter{
          float: left;
          color: #fff;
          font-family: 'Reservation Wide';
          margin-top: 4px;
          margin-right: 5px;
          font-size: 94px;
          letter-spacing: 5.28px;
          line-height: 82px;
        }
      }
      .column-information{
        font-family: 'Proxima Nova';
        font-size: 17px;
        color: #FFFFFF;
        text-align: justify;
        @media(min-width: 768px){
          font-size: 21px;
        }
      }
      .content-line-bottom{
        border-bottom: 1px dashed #FFF;
      }
      .column-information-strong{
        border-top: 1px solid #FFF;
        border-bottom: 1px solid #FFF;
        font-family: 'Reservation Wide';
        font-size: 20px;
        letter-spacing: 2.6px;
        text-transform: uppercase;
        color: #FFFFFF;
        @media(min-width: 768px){
          font-size: 30px;
          letter-spacing: 3.6px;
        }
      }
    }
  }

}
.template-article, #nosotros, #aviso-de-privacidad, #terminos-y-condiciones{
  background: url('https://cdn.shopify.com/s/files/1/0501/8127/5807/files/bg-body.jpg?v=1621900982');
  background-size: cover;
  overflow-x: hidden;
  background-position: center;
  .box-date{
    border: 3px solid #333333;
    border-bottom: 0px;
    padding: 5px 15px;
    .date{
      font-family: 'BasierSquareMono Regular';
      font-size: 13px;
      text-transform: uppercase;
      color: #000000;
    }
  }
  .content-image-article{
    img{
      width: 100%;
      height: auto;
    }
  }
  [data-image-loading-animation]{
    background-color: transparent !important;
  }
  .search-bar{
    right: 27px;
  }
  .site-header__logo-image{
    img{
      padding: 15px;
    }
  }
  .site-header{
    padding: 0 12px;
    background-color: transparent;
    @media(min-width: 768px){
      padding: 0 12px;
    }
    @media(min-width: 992px){
      padding: 0 30px;
    }
    .grid{
      background-color: transparent;
      border: 3px solid #333333;
      padding: 5px 15px;
    }
  }
  .main-content{
    padding: 0px;
    .box-title-header{
      background: #000000;
      font-family: 'Presicav';
      font-size: 13px;
      letter-spacing: 2.6px;
      text-transform : uppercase;
      color : #FFFFFF;
      padding: 8px;
      @media(min-width: 768px){
        font-size: 13px;
        letter-spacing: 2.6px;
      }
      @media(min-width: 992px){
        font-size: 12px;
        letter-spacing : 2.4px;
      }
    }
  }
  .box-tags-cat{
    border: 3px solid #333;
    border-bottom: 0px;
    .box-filters:last-child{
      border: none;
    }
    .box-filters{
      border-right: 3px solid #333;
      padding: 5px 15px;
      a{
        font-family: 'Darwin Ess Rd';
        font-weight : bold;
        font-size: 19px;
        color: #FFF;
        text-decoration: none;
        @media(min-width: 768px){
          font-size: 19px;
        }
        @media(min-width: 992px){
          font-size: 19px;
        }
        &:hover{
          text-decoration: none;
        }
      }
      &.active{
        &.Cuerpo{
          background: #56a150;
        }
        &.Espiritu{
          background: #b73a97;
        }
        &.Mente{
          background: #e38016;
        }
        a{
          color: #FFF;
        }
      }
    }
  }
  .search-box{
    position: absolute;
    width: auto;
    right: 67px;
    top: 38px;
  }
  .box-menu-blog-collapse{
    flex: 0 0 auto;
    width: 80px;
    @media (min-width: 576px){
      width: 125px;
    }
    @media(min-width: 768px){
      width: 125px;
    }
    @media(min-width: 992px){
      width: 142px;
    }
    .wrapper{
      @media (max-width: 575px){
        padding: 0 !important;
      }
    }
  }
  .box-menu-blog-collapse{
    .nav-bar{
      border: 3px solid #333;
      border-bottom: 0;
      background-color: transparent;
    }
  }
  .content-box-logo-center{
    width: calc(100% - 152px);
    padding-right: 0px;
    @media (min-width: 576px){
      width: calc(100% - 235px);
    }
    @media(min-width: 768px){
      width: calc(100% - 235px);
    }
    @media(min-width: 992px){
      width: 100%;
      padding-right: 12px;
    }
  }
  .search-mobile-collapse{
    .content-box{
      border: 3px dashed #333;
      margin: 0px 0 20px;
      padding: 25px 0;
      border-top: 3px solid;
      .site-header__search{
        max-width: 80%;
        margin: 0 auto;
      }
      input{
        border: 3px solid #333;
        background: 0 0;
      }
    }
    .button-submit-search{
      display: none;
    }
    .input-group.search-bar{
      right: 0;
    }
  }
  .content-box-search-mobile{
    width: 98px;
    border-top: 3px solid #333;
    border-right: 3px solid #333;
    @media (max-width: 575px){
      width: 60px;
    }
    @media (min-width: 992px){
      display: none;
    }
    svg{
      @media (max-width: 575px){
        width: 20px;
      }
    }
  }
  .box-menu-logo{
    .box-logo-search{
      border-top: 3px solid #333;
      border-right: 3px solid #333;
      height: 100%;
      .site-header__logo{
        justify-content: center;
        width: 100%;
        padding: 15px;
        @media(min-width: 768px){
        }
        @media(min-width: 992px){
          padding-right: 150px;
        }
      }
      .search-bar{
        input{
          border: 3px solid #333;
          background: transparent;
        }
      }
    }
  }
  .button-submit-search{
    &:hover{
      background: transparent;
    }
  }
  .box-date-header{
    border-top: 3px solid #333;
    border-right: 3px solid #333;
    border-left: 3px solid #333;
    padding-left: 15px;
    padding-right: 15px;
    @media(min-width: 768px){
    }
    @media(min-width: 992px){
      border-left: none;
    }
    .box-date-header-content{
      font-family: 'BasierSquareMono Regular';
      font-size: 8px;
      text-transform: uppercase;
      color: #000000;
      @media (min-width: 576px){
        font-size: 14px;
      }
      @media(min-width: 768px){
        font-size: 14px;
      }
      @media(min-width: 992px){
        font-size: 13px;
      }
    }
  }
  .container-article-element{
    border: 3px solid #000;
    padding: 40px 15px;
    p{
      padding: 0px !important;
    }
    .container-box-general{
      border: 3px solid #000;
      .box-date-header-article{
        border-bottom: 3px solid #000;
        font-size: 13px;
        color: #12100B;
        font-family: 'BasierSquareMono Regular';
        text-transform: uppercase;
        padding: 10px;

      }
      .container-title-article{
        font-size: 86px;
        font-family: 'BebasNeueBold';
        color: #D53842;
        @media(min-width: 768px){
          font-size: 86px;
        }
        @media(min-width: 992px){
          font-size: 86px;
        }
      }
      .button-more-view-article{
        font-family: 'Tungsten';
        font-size: 30px;
        line-height: 29px;
        text-transform: uppercase;
        color: #000;
        padding-top: 40px;
        @media(min-width: 768px){
          font-size: 30px;
          line-height: 29px;
          width: 50%;
          margin: 0 auto;
        }
        @media(min-width: 992px){
          font-size: 42px;
          line-height: 42.17px;
          width: 100%;
          padding-top: 60px;
        }

      }
      .content-txt-blog{
        font-family: 'Proxima Nova';
        font-size: 16px;
        line-height: 17px;
        color: #12100B;
        letter-spacing: -0.4px;
        padding: 30px;
        text-align: justify;
        @media(min-width: 768px){
          font-size: 16px;
          line-height: 17px;
        }
        @media(min-width: 992px){
          letter-spacing: 0px;
          font-size: 14px;
          line-height: 18px;
        }
        @media(min-width: 1500px){
          font-size: 18px;
          line-height: 22px;
        }
        .rte-setting{
          text-align: justify !important;
        }
        p{
          text-align: justify;
          font-family: 'Proxima Nova';
          font-size: 16px;
          line-height: 17px;
          letter-spacing: -0.4px;
          color: #12100B;
          @media(min-width: 768px){
            font-size: 16px;
            line-height: 17px;
          }
          @media(min-width: 992px){
            letter-spacing: 0px;
            font-size: 14px;
            line-height: 18px;
          }
          @media(min-width: 1500px){
            font-size: 18px;
            line-height: 22px;
          }
        }
        strong{
          font-family: 'Proxima Nova Bold';
        }
        .first-column-txt{
          .rte-setting{
            &::first-letter{
              float: left;
              color: #12100B;
              font-family: 'Granjon LT';
              margin-top: 0px;
              margin-right: 5px;
              font-size: 75px;
              line-height: 51.65px;
            }
          }
        }
        .first-column-txt{
          .rte-setting{
            p:first-child{
              &::first-letter{
                font-size: 285%;
                float: left;
                color: #12100B;
                font-family: 'Inconsolata';
                margin-top: 10px;
                margin-right: 5px;
              }
            }
          }
        }
        .second-column-txt{
          .box-title-strong-index{
            font-size: 32px;
            font-family: 'BebasNeueBold';
            color: #12100B;
            line-height: 40px;
            background: #FFD956;
            padding: 20px;
            border-radius: 15px;
            margin-bottom: 20px;
            p{
              margin: 0px !important;
            }
          }
        }
      }
      .title_lines {
        position: relative;
        font-size: 30px;
        z-index: 1;
        overflow: hidden;
        text-align: center;
        font-family: 'Presicav';
        color: #000;
        font-size : 16px;
        letter-spacing : 3.2px;
        width: 90%;
        margin: 0 auto;
        @media(min-width: 768px){
          font-size : 16px;
          letter-spacing : 3.2px;
        }
        @media(min-width: 992px){
          font-size : 16px;
          letter-spacing : 3.2px;
        }
      }
      .title_lines:before, .title_lines:after {
        position: absolute;
        top: 51%;
        overflow: hidden;
        width: 48%;
        height: 2px;
        content: '\a0';
        background-color: #333;
        margin-left: 2%;
      }
      .title_lines:before {
        margin-left: -50%;
        text-align: right;
      }
      .container-title-article{
        font-family: 'Darwin Ess Rd Black';
        font-size: 40px;
        line-height: 45.79px;
        letter-spacing: -1.25px;
        @media(min-width: 768px){
          font-size: 75px;
          line-height: 65.79px;
          letter-spacing: -2.25px;
        }
        @media(min-width: 992px){
          font-size: 90px;
          line-height: 90px;
          letter-spacing: -4.38px;
        }

      }
      .container-subtitle-article{
        font-family: 'Darwin Ess Rd';
        font-size: 45px;
        line-height: 45.79px;
        letter-spacing: -1.25px;
        @media(min-width: 768px){
          font-size: 75px;
          line-height: 77px;
          letter-spacing: -2.25px;
        }
        @media(min-width: 992px){
          font-size: 70px;
          line-height: 70px;
          letter-spacing: -4.38px;
        }

      }
      .content-box-column-text{
        .column-first-video{
          &::first-letter{
            float: left;
            color: #12100B;
            font-family: 'Granjon LT';
            margin-top: 0px;
            margin-right: 5px;
            font-size: 75px;
            line-height: 51.65px;
          }
        }
        .column-text{
          text-align: justify;
          font-family: 'Proxima Nova';
          font-size: 16px;
          line-height: 17px;
          color: #12100B;
          @media(min-width: 768px){
            font-size: 16px;
            line-height: 17px;
          }
          @media(min-width: 992px){
            font-size: 14px;
            line-height: 18px;
          }
          @media(min-width: 1500px){
            font-size: 18px;
            padding: 30px;
            line-height: 22px;
          }
          p{
            font-family: 'Proxima Nova';
            font-size: 16px;
            line-height: 17px;
            color: #12100B;
            @media(min-width: 768px){
              font-size: 16px;
              line-height: 17px;
            }
            @media(min-width: 992px){
              font-size: 14px;
              line-height: 18px;
            }
            @media(min-width: 1500px){
              font-size: 18px;
              line-height: 22px;
            }
          }
        }
      }
    }
    .image-blog{
      .content-box-image{
        border-top: 3px solid #000;
      }
    }
    .button-more-view-article{
      .button-link-view-more{
        font-family: 'Tungsten Bold';
        color: #000;
        border: 3px solid #000;
        padding: 0px 5px;
        font-size: 26px;
        line-height: 15.42px;
        text-decoration: none;
        cursor: pointer;
        @media(min-width: 768px){
          font-size: 26px;
          line-height: 15.42px;
        }
        @media(min-width: 992px){
          font-size: 42px;
          line-height: 24.17px;
        }
        span{
          border-left: 3px solid #000;
          padding: 0px 6px;
          svg{
            margin-left: 5px;
          }
        }
      }
      .button-link-view-more.active{
        span{
          svg{
            transform: rotate(180deg);
          }
        }
      }
    }
    .box-shared{
      border: 3px solid #000;
      border-top: 0;
      .social-sharing{
        a{
          padding: .375rem 4px;
          &:hover, &:focus, &:active{
            background-color: transparent;
          }
        }
      }
    }
  }
  .content-image-article-info{
    video{
      object-fit: cover;
      width: 100%;
      top: 0;
      left: 0;
    }
    img{
      width: 100%!important;
      padding: 0!important;
      max-width: 100%;
      height: auto;
    }
  }
  .content-cintillo-loop-infografia{
    .box-cintillo-loop-infografia{
      background: #000000;
      font-family: 'Darwin Ess Rd Black';
      font-size: 53px;
      line-height: 52.84px;
      text-transform: uppercase;
      color: #FFFFFF;
      padding: 10px;
      @media(min-width: 768px){
        font-size: 59px;
        line-height: 62.84px;
      }
      @media(min-width: 992px){
        font-size: 63px;
        line-height: 62.84px;
      }
    }
  }
}
