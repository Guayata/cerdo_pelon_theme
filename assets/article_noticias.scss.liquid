/*========================= Footer ==============================*/
#shopify-section-footer{
  margin-right: 12px;
  margin-left: 12px;
  @media(min-width: 768px){
    margin-right: 12px;
    margin-left: 12px;
  }
  @media(min-width: 992px){
    margin-right: 0px;
    margin-left: 0px;
  }
  .page-width-padding{
    padding-left: 30px;
    padding-right: 30px;
    @media(min-width: 768px){
      padding-left: 30px;
      padding-right: 30px;
    }
    @media(min-width: 992px){
      padding-left: 50px;
      padding-right: 50px;
    }
  }
  footer{
    .box-title-group{
