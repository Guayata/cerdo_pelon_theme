.section-column-images-hover{
  background: #ebebeb;
  padding: 50px 0px;
  @media(min-width: 992px){
    padding: 50px 0px;
  }
  .content-image-hover{
    padding: 0px 5px;
  }
  .overlay-outer {
    cursor: pointer;
    position: relative;
    width: 100%;
    max-width: 100%;
    margin: auto;
    overflow: hidden;
  }

  .overlay {
    background: rgba(254,74,56,0.7);
    position: absolute;
    height: 100%;
    width: 100%;
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
    opacity: 0;
    transition: all 0.4s ease-in-out 0s;
  }

  a:hover .overlay {
    opacity: 1;
  }
  .content-details {
    position: absolute;
    text-align: center;
    padding-left: 1em;
    padding-right: 1em;
    width: 100%;
    top: 50%;
    left: 50%;
    opacity: 0;
    transform: translate(-50%, -50%);
    transition: all 0.3s ease-in-out 0s;
  }

  .overlay-outer:hover .content-details {
    top: 50%;
    left: 50%;
    opacity: 1;
  }
  .content-text{
    color: #FFF;
    font-size: 18px;
    line-height: 20px;
    font-family: 'Futura Medium';
    @media(min-width: 768px){
      font-size: 28px;
      line-height: 30px;
    }
    span{
      font-family: 'Futura LT Bold';
    }
  }

  .fadeIn_bottom {
    top: 80%;
  }
}
