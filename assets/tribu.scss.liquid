      border-radius: 46px;
      @media(min-width: 992px){
        padding: 50px;
        width: 800px;
        margin: 100px auto 0;
        background: #ffcf00;
        box-shadow: 10px 13px 1px 0px rgba(229,229,229,0.75);
        -webkit-box-shadow: 10px 13px 1px 0px rgba(229,229,229,0.75);
        -moz-box-shadow: 10px 13px 1px 0px rgba(229,229,229,0.75);
        border-radius: 46px;
      }
      .title-contact-tribu{
        font-family: 'Darwin Ess Rd Bold';
        font-size: 18px;
        line-height: 26px;
        margin-bottom: 40px;
        @media(min-width: 992px){
          font-size : 24px;
          line-height: 31px;
        }
      }
      .content-input{
        margin-bottom: 10px;
        padding: 0 6px;
        input, select{
          background: rgba(255,215,51,0.45);
          border-radius:  5px;
          height: 45px;
          color: #666666;
          border: 1px solid #000000;
          letter-spacing: 0 !important;
          font-family: 'Oliviar Sans Ultra Light';
          font-size : 11px !important;
          line-height: 14px;
          @media(min-width: 992px){
            font-size : 13px !important;
            line-height: 15px;
          }

          &::placeholder{
            color: #666666;
            opacity: 0.4;
            letter-spacing: 0 !important;
          }
          &:focus{
            border-color: #07e401;
            box-shadow: 0 0 0 0.1rem rgba(7, 228, 1, .15);
          }
        }
        .checkbox {
          width: 100%;
          margin: 15px auto;
          position: relative;
          display: block;
          padding: 0;
        }

        .checkbox input[type="checkbox"] {
          width: auto;
          opacity: 0.00000001;
          position: absolute;
          left: 0;
          margin-left: -20px;
        }
        .checkbox label {
          position: relative;
        }
        .checkbox label:before {
          content: '';
          position: absolute;
          left: 0;
          top: 0;
          margin: 4px;
          width: 26px;
          height: 26px;
          transition: transform 0.28s ease;
          border-radius: 3px;
          background: rgba(255,215,51,0.45);
          border: 1px solid #000;
        }
        .checkbox label:after {
          content: '';
          display: block;
          width: 10px;
          height: 5px;
          border-bottom: 2px solid #07e401;
          border-left: 2px solid #07e401;
          -webkit-transform: rotate(-45deg) scale(0);
          transform: rotate(-45deg) scale(0);
          transition: transform ease 0.25s;
          will-change: transform;
          position: absolute;
          top: 14px;
          left: 12px;
        }
        .checkbox input[type="checkbox"]:checked ~ label::before {
          color: #07e401;
        }

        .checkbox input[type="checkbox"]:checked ~ label::after {
          -webkit-transform: rotate(-45deg) scale(1);
          transform: rotate(-45deg) scale(1);
        }

        .checkbox label {
          min-height: 34px;
          display: block;
          padding-left: 40px;
          margin-bottom: 0;
          font-weight: normal;
          cursor: pointer;
          vertical-align: sub;
          font-family: 'Oliviar Sans Ultra Light';
          font-size : 13px;
          line-height: 15px;
          letter-spacing: 0 !important;
          text-transform: initial;
        }
        .checkbox label span {
          position: absolute;
          top: 50%;
          -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
        }
        .checkbox input[type="checkbox"]:focus + label::before {
          outline: 0;
        }
      }
      .content-button-submit {
